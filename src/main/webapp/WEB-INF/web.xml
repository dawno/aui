  	<display-name>TemporaryController</display-name>
  	<description></description>
  	<servlet-class>com.aui.web.controller.TemporaryController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterController</servlet-name>
  	<url-pattern>/RegisterController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeletionController</servlet-name>
  	<url-pattern>/DeletionController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeletionRegister</servlet-name>
  	<url-pattern>/DeletionRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReadController</servlet-name>
  	<url-pattern>/ReadController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateController</servlet-name>
  	<url-pattern>/UpdateController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TemporaryController</servlet-name>
  	<url-pattern>/TemporaryController</url-pattern>
