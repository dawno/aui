<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterController</servlet-name>
  	<display-name>RegisterController</display-name>
  	<description></description>
  	<servlet-class>com.aui.web.controller.RegisterController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeletionController</servlet-name>
  	<display-name>DeletionController</display-name>
